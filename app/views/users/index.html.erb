<% provide(:title, 'Users') %>
<div class="form content">
  <h1 class="text-center">List of Users</h1>
  <div class="container">
    <ul class="users">
      <% @users.each do |user| %>
        <li>
        <%= gravatar_for user, size:52 %>
        <%= link_to user.name, user%>
        </li>
      <% end %>
    </ul>
  </div>
</div>